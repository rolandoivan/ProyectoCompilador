var int i,j,p;
var int Arreglo[11];
var int Matriz[11][8];

def int fact(int j){
    var int i;
    i=j+(p-j*2+j);
    if (j==1){
        return(j);
    }
    else{
        return(j*fact(j-1));
    }
}

def void inicia(int y){
    var int x;
    x=0;
    while (x<11){
        Arreglo[x] = y * x;
        x=x+1;
    }
}

main {
    read(p);
    j=p*2;i=1;
    inicia(p*j-5);
    do{
        Arreglo[i] = Arreglo[i] * fact(Arreglo[i]-p);
        Matriz[i][Arreglo[i+p]*p] = Arreglo[i];
        i = i + 1;
    } while (i<11);

    while (i>=1){
        print("resultado",Arreglo[i],fact(i+2));
        print(Matriz[i][p*2-i]);
        i = i - 1;
    }
}
